    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-rebuild": "^2.3.4",
    "electron-reloader": "^1.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
